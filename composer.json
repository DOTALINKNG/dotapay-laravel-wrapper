{
  "name": "dotapay/laravel-sdk",
  "description": "Laravel SDK for integrating with DotaPay (customers, payment, settlements) using Private Key auth.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {"name": "DotaPay", "email": "engineering@dotapay.ng"}
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "psr/log": "^1.1|^2.0|^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "DotaPay\\LaravelSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotaPay\\LaravelSdk\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DotaPay\\LaravelSdk\\DotaPayServiceProvider"
      ],
      "aliases": {
        "Dotapay": "DotaPay\\LaravelSdk\\Facades\\Dotapay"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
